#, no-wrap
msgid "*-o, --postlogin*:"
msgstr "*-o, --postlogin*:"

#. type: Plain text
#: src/man/authselect.8.adoc:139
#, no-wrap
msgid "Print postlogin content.\n"
msgstr "포스트로그인 내용 인쇄.\n"

#. type: Labeled list
#: src/man/authselect.8.adoc:140
#, no-wrap
