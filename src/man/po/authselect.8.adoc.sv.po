#: /home/pbrezina/workspace/authselect/src/man/authselect.8.adoc:343
#, no-wrap
msgid "SEE ALSO"
msgstr "SE ÄVEN"

#. type: Plain text
#: /home/pbrezina/workspace/authselect/src/man/authselect.8.adoc:344
msgid ""
"authselect-profiles(5), authselect-migration(7), nsswitch.conf(5), PAM(8)"
msgstr ""
"authselect-profiles(5), authselect-migration(7), nsswitch.conf(5), PAM(8)"
