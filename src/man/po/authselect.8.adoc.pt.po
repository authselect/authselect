msgid "authselect - select system identity and authentication sources."
msgstr "authselect - seleciona identidade do sistema e fontes de autenticação."

#. type: Title -
#: src/man/authselect.8.adoc:11
#, no-wrap
msgid "SYNOPSIS"
msgstr "RESUMO"

#. type: Plain text
#: src/man/authselect.8.adoc:13
